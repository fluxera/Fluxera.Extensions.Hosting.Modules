{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"Hosting": {
		"Modules": {
			"AspNetCore": {
				"BaseUrl": "https://localhost:5001/",
				"HttpApi": {
					"Descriptions": {
						"v1": {
							"Title": "WebSample API",
							"Description": "This is the WebSample v1.0 API."
						},
						"v2": {
							"Title": "WebSample API",
							"Description": "This is the WebSample v2.0 API."
						}
					},
					"Versioning": {
						"Enabled":  true
					},
					"Swagger": {
						"Enabled": true
					}
				},
				"Authentication": {
					"DefaultScheme": "Bearer",
					"Schemes": {
						"JwtBearer": {
							"Default": {
								"Authority": "MyAuthority-1",
								"SigningKey": "MySigningKey-1"
							}
						}
					}
				},
				"Cors": {
					"Origins": [
						"http://localhost:5000",
						"https://localhost:5001"
					]
				}
			},
			"OpenTelemetry": {
				"Endpoint": "https://otlp-service:4317"
			},
			"Persistence": {
				"Repositories": {
					"Test": {
						"ProviderName": "InMemory",
						"DatabaseName": "database",
						"DatabaseNamePrefix": "prefix",
						"ConnectionStringName": "Default",
						"Settings": {
							"TestSetting": "TestValue"
						}
					}
				}
			}
		}
	},
	"ConnectionStrings": {
		"Default": "localhost"
	}
}
