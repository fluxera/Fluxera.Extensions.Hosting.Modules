{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Warning",
			"Override": {
				"Microsoft.AspNetCore": "Warning"
			}
		}
	},
	"AllowedHosts": "*",
	"Hosting": {
		"Modules": {
			"Persistence": {
				"Repositories": {
					"Default": {
						"ProviderName": "EntityFrameworkCore",
						"DatabaseName": "database",
						"DatabaseNamePrefix": "prefix",
						"ConnectionStringName": "Default"
					}
				}
			}
		}
	},
	"ConnectionStrings": {
		"Default": "localhost"
	}
}
