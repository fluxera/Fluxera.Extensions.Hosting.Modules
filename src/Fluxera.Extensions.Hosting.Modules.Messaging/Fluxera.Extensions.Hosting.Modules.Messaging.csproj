<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
	</PropertyGroup>

	<PropertyGroup>
		<Title>Fluxera.Extensions.Hosting.Modules.Messaging</Title>
		<Description>A module that enables messaging based on MassTransit.</Description>
		<PackageTags>library;extensions;hosting;modular;messaging;messagebus;masstransit</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\..\README.md" Link="Properties\README.md">
			<Pack>true</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\..\icon.png" Link="Properties\icon.png">
			<Pack>true</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Fluxera.Enumeration.SystemTextJson" Version="8.0.0" />
		<PackageReference Include="Fluxera.Extensions.Common" Version="8.0.0" />
		<PackageReference Include="Fluxera.Extensions.DataManagement.Abstractions" Version="8.0.0" />
		<PackageReference Include="Fluxera.Extensions.Hosting" Version="8.0.0" />
		<PackageReference Include="Fluxera.Extensions.Localization" Version="8.0.0" />
		<PackageReference Include="Fluxera.Extensions.Validation" Version="8.0.0" />
		<PackageReference Include="Fluxera.Spatial.SystemTextJson" Version="8.0.0" />
		<PackageReference Include="Fluxera.StronglyTypedId.SystemTextJson" Version="8.0.0" />
		<PackageReference Include="Fluxera.ValueObject.SystemTextJson" Version="8.0.0" />
		<PackageReference Include="GitVersion.MsBuild" Version="5.12.0">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
		<PackageReference Include="IdentityModel" Version="6.2.0" />
		<PackageReference Include="JetBrains.Annotations" Version="2023.3.0" />
		<PackageReference Include="MassTransit" Version="8.1.2" />
		<PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="7.0.3" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\Fluxera.Extensions.Hosting.Modules.Configuration\Fluxera.Extensions.Hosting.Modules.Configuration.csproj" />
	  <ProjectReference Include="..\Fluxera.Extensions.Hosting.Modules.DataManagement\Fluxera.Extensions.Hosting.Modules.DataManagement.csproj" />
	  <ProjectReference Include="..\Fluxera.Extensions.Hosting.Modules.OpenTelemetry\Fluxera.Extensions.Hosting.Modules.OpenTelemetry.csproj" />
	  <ProjectReference Include="..\Fluxera.Extensions.Hosting.Modules.Principal\Fluxera.Extensions.Hosting.Modules.Principal.csproj" />
	</ItemGroup>

</Project>
