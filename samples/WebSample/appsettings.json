{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"Hosting": {
		"Modules": {
			"AspNetCore": {
				"BaseUrl": "https://localhost:5001/",
				"Cors": {
					"Origins": [
						"http://localhost:5000",
						"https://localhost:5001"
					]
				},
				"Swagger": {
					"Enabled": true
					//"Descriptions": {
					//	"v1": {
					//		"Version": "1.0",
					//		"Title": "Customers API",
					//		"Description": "This is the customers API."
					//	},
					//	"v2": {
					//		"Version": "2.0",
					//		"Title": "Customers API",
					//		"Description": "This is the customers API."
					//	}
					//} 
				} 
			},
			"OpenTelemetry": {
				"Endpoint": "https://otlp-service:4317"
			},
			"Authentication": {
				"DefaultScheme": "Bearer",
				"Schemes": {
					"JwtBearer": {
						"Default": {
							"Authority": "MyAuthority-1",
							"SigningKey": "MySigningKey-1"
						}
					}
				}
			},
			"Persistence": {
				"Repositories": {
					"Test": {
						"ProviderName": "InMemory",
						"DatabaseName": "database",
						"DatabaseNamePrefix": "prefix",
						"ConnectionStringName": "Default",
						"Settings": {
							"TestSetting": "TestValue"
						}
					}
				}
			}
		}
	},
	"ConnectionStrings": {
		"Default": "localhost"
	}
}
